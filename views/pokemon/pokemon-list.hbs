
<div class="row">
    {{#each pokemon}}
    <div class="col-md-4">
        <div class="card" style="width: 18rem;">
            <img src="{{this.image}}" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <img src="{{this.url}}" alt="">
            </div>
        </div>
    </div>
    {{/each}}